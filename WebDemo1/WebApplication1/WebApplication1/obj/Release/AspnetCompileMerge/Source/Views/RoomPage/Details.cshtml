@model WebApplication1.Models.HistoryModels

@{
    ViewBag.Title = "KTC会议室";
}

<div class="meet_container" style="height:500px;">
    <h3>查看会议记录</h3>
    <hr />
    <dl class="dl-horizontal" style="font-size:20px;">
        <dt>
            @Html.DisplayName("会议室名称:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RoomName)
        </dd>

        <dt>
            @Html.DisplayName("预约人:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Person)
        </dd>

        <dt>
            @Html.DisplayName("参会人数:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Peopel)
        </dd>

        <dt>
            @Html.DisplayName("预约人:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Coustomer)
        </dd>

        <dt>
            @Html.DisplayName("开始时间:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StartTime)
        </dd>

        <dt>
            @Html.DisplayName("接收时间:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.EndTime)
        </dd>

        <dt>
            @Html.DisplayName("备注:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Tips)
        </dd>

    </dl>
    <br />
    <div style="font-size:18px;padding-left: 100px;">
        @Html.ActionLink("返回", "Index", new { @class = "btn btn-default" })
    </div>
</div>
